/*   Updated: 2014/11/10 14:48:20 by tfleming         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

static void		*ft_calloc(void const *thing, size_t size)
{
	
}

t_list			*ft_lstnew(void const *content, size_t content_size)
{
	t_list	*new;

	if (!(new = malloc(sizeof(t_list))))
		return (NULL);
	new->content = ft_calloc(content);
	new->content_size = (content ? content_size : 0);
	new->next = NULL;
	return (new);
}
