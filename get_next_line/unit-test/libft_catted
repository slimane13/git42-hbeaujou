/*   Updated: 2014/11/08 19:14:58 by tfleming         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void			ft_putstr_fd(char const *source, int file_descriptor)
{
	write(file_descriptor, source, ft_strlen(source));
}
